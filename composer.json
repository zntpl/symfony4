{
    "type": "zntpl",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",

        "znsandbox/sandbox": "dev-master",

        "znbundle/article": "dev-master",
        "znbundle/messenger": "dev-master",
        "znbundle/queue": "dev-master",
        "znbundle/reference": "dev-master",
        "znbundle/storage": "dev-master",
        "znbundle/user": "dev-master",

        "zncore/db": "dev-master",
        "zncore/ddd": "dev-master",

        "znlib/rest": "dev-master",
        "znlib/socket": "dev-master",

        "zncrypt/jwt": "dev-master",
        "zncrypt/pki": "dev-master",
        "zncrypt/tunnel": "dev-master",

        "php7lab/bundle": "v0.0.1",
        "php7lab/web": "v0.0.1",

        "symfony/asset": "4.*",
        "symfony/console": "4.*",
        "symfony/expression-language": "4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.*",
        "symfony/framework-bundle": "4.*",
        "symfony/http-client": "4.*",
        "symfony/intl": "4.*",
        "symfony/maker-bundle": "^1.13",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "4.*",
        "symfony/security-bundle": "4.*",
        "symfony/serializer": "4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.*",
        "symfony/twig-bundle": "4.*",
        "symfony/validator": "4.*",
        "symfony/web-link": "4.*",
        "symfony/yaml": "4.*",
        "sensio/framework-extra-bundle": "^5.1",
        "jms/serializer-bundle": "^3.4",
        "friendsofsymfony/rest-bundle": "^2.5",
        "friendsofsymfony/user-bundle": "^2.1"
    },
    "require-dev": {
        "zntool/dev": "dev-master",
        "zntool/test": "dev-master",
        "zndoc/rest-api": "v0.0.1",
        "symfony/debug-pack": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.*"
    },
    "config": {
        "secure-http": false,
        "process-timeout": 1800,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.*"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "https://github.com/znsandbox/sandbox.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/web.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/bundle.git"
        }
    ]
}
